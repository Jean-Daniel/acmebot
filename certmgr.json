{
  "account": {
    "email": "sysadmin@xooloo.com"
  },
  "settings": {
    "log_level": "debug",
    "color_output": true,
    "key_size": 4096,
    "key_curve": "secp384r1",
    "dhparam_size": 512,
    "ecparam_curve": null,
    "file_user": "jddupas",
    "file_group": "admin",
    "ocsp_must_staple": false,
    "ocsp_responder_urls": [
      "http://ocsp.int-x3.letsencrypt.org"
    ],
    "ct_submit_logs": [
      "google_icarus",
      "google_pilot"
    ],
    "renewal_days": 30,
    "max_authorization_attempts": 2,
    "acme_directory_url": "https://acme-v02.api.letsencrypt.org/directory",
    "verify": [ 443 ]
  },
  "directories": {
    "pid": "acme/run",
    "log": "acme/log",
    "link": "acme/live",
    "resource": "acme/resource",

    "private_key": "acme/keys",
    "full_key": null,

    "certificate": "acme/certs",
    "full_certificate": "acme/certs",
    "chain": "acme/certs",

    "http_challenge": "/usr/local/var/www/.well-known/acme-challenge",

    "param": "acme/params",
    "ocsp": "acme/ocsp",
    "sct": "acme/scts/{name}/{key_type}",

    "archive": "acme/archive/"
  },
  "services": {
    "dovecot": "systemctl restart dovecot",
    "mysql": "systemctl reload mysql",
    "nginx": "systemctl reload nginx",
    "postfix": "systemctl reload postfix",
    "postgresql": "systemctl reload postgresql",
    "swanctl": "systemctl reload strongswan-swanctl",
    "slapd": "systemctl restart slapd"
  },
  "hooks": {
    "set_http_challenge": null,
    "clear_http_challenge": null,
    "private_key_installed": null,
    "certificate_installed": null,
    "full_certificate_installed": null,
    "chain_installed": null,
    "full_key_installed": null,
    "params_installed": null,
    "sct_installed": null,
    "ocsp_installed": null,
    "certificates_updated": {
      "args": [ "echo", "{certificates}" ]
    }
  },
